{
  "name": "base",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=release nest start --watch",
    "start:dev": "cross-env NODE_ENV=debug nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create ./src/migrations/create-post-table",
    "typeorm:migrate": "npx typeorm-ts-node-esm migration:generate ./src/migrations/update-post-table -d ./src/config/database/app.dataSource.ts",
    "migrations-run": "npx typeorm-ts-node-commonjs migration:run -d ./src/config/database/app.dataSource.ts",
    "migrations-revert": "npm run typeorm:cli -- migration:revert"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/web3": "^1.2.2",
    "abi-decoder": "^2.4.0",
    "bcrypt": "^5.0.1",
    "bull": "^4.10.0",
    "cache-manager": "^3.6.3",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ejs": "^3.1.8",
    "ip-address": "^8.1.0",
    "ip-num": "^1.4.1",
    "js-big-decimal": "^1.3.10",
    "js-sha512": "^0.8.0",
    "jsonrpc-ts": "^0.2.1",
    "mongoose": "^6.6.5",
    "mv": "^2.1.1",
    "nestjs-ftp": "^0.1.13",
    "nestjs-i18n": "^9.1.10",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "phone": "^3.1.28",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "tronweb": "^4.2.0",
    "typeorm": "^0.3.9",
    "typeorm-extension": "^2.4.1",
    "ua-parser-js": "^1.0.2",
    "web3": "^1.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^3.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
